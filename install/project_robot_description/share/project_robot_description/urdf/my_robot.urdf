<?xml version="1.0"?>
<robot name="my_robot">

    <material name="green">
        <color rgba="0 1 0 1"/>
    </material>
    <material name="red">
        <color rgba="0.5 0 0 1"/>
    </material>
    <link name="fixed_frame">
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </link>
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.6 0.4 0.2"/>
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
            <material name="green"/>
        </visual>
    </link> 
    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
            <material name="red"/>
        </visual>
    </link>
    <link name="right_wheel">
        <visual>
            <geometry>
              <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <origin xyz="0 0 0" rpy="1.57 0 0"/>
            <material name="red"/>
        </visual>
    </link>
    <link name="caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="red"/>
        </visual>
    </link> 
    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <axis xyz="0 1 0"/>
        <origin xyz="-0.20 0.225 0" rpy="0 0 0"/>
    </joint>
    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <axis xyz="0 1 0"/>
        <origin xyz="-0.2 -0.225 0" rpy="0 0 0"/>
    </joint>
    <joint name="caster_wheel_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_wheel"/>
        <origin xyz="0.2 0.0 -0.05" rpy="0.0 0.0 0.0"/>
    </joint>
    <joint name="fixed_frame_joint" type="fixed">
        <parent link="fixed_frame"/>
        <child link="base_link"/>
        <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/>
    </joint>
    
    
    
</robot>